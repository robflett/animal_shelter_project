<form method="POST" action="/owners/<%= @owner.id %>">
<label for="name">Name: </label>
<input type="text"
id="name"
value="<%= @owner.name %>"
name="name"/>


<label for="address">Address: </label>
<input type="text"
id="address"
value="<%= @owner.address %>"
name="address" />





<input type="submit" value="Update Owner" />


</form>